
<!DOCTYPE html>
<!--[if lt IE 9]><html class="lte-ie8" lang="{{localeLanguage}}"><![endif]-->
<!--[if gt IE 8]><!--><html lang="{{localeLanguage}}"><!--<![endif]-->
  <head>
    <meta charset="utf-8" />
    <title>{{windowTitle}}</title>

    <!--[if gt IE 8]><!--><link rel="stylesheet" media="screen" href="{{themeBaseUrl}}/stylesheets/govuk-template.css"/><!--<![endif]-->
    <!--[if IE 6]><link rel="stylesheet" media="screen" href="{{themeBaseUrl}}/stylesheets/govuk-template-ie6.css"/><![endif]-->
    <!--[if IE 7]><link rel="stylesheet" media="screen" href="{{themeBaseUrl}}/stylesheets/govuk-template-ie7.css"/><![endif]-->
    <!--[if IE 8]><link rel="stylesheet" media="screen" href="{{themeBaseUrl}}/stylesheets/govuk-template-ie8.css"/><![endif]-->
    <link rel="stylesheet" media="print" href="{{themeBaseUrl}}/stylesheets/govuk-template-print.css"/>

    <!--[if IE 8]><link rel="stylesheet" media="all" href="{{themeBaseUrl}}/stylesheets/fonts-ie8.css"/><![endif]-->
    <!--[if gte IE 9]><!--><link rel="stylesheet" media="all" href="{{themeBaseUrl}}/stylesheets/fonts.css"/><!--<![endif]-->
    <!--[if lt IE 9]><script src="{{themeBaseUrl}}/javascripts/ie.js"></script><![endif]-->

    <link rel="shortcut icon" href="{{themeBaseUrl}}/images/favicon.ico" type="image/x-icon" />

    <link rel="mask-icon" href="{{themeBaseUrl}}/images/gov.uk_logotype_crown.svg" color="#0b0c0c">
    <link rel="apple-touch-icon" sizes="180x180" href="{{themeBaseUrl}}/images/apple-touch-icon-180x180.png">
    <link rel="apple-touch-icon" sizes="167x167" href="{{themeBaseUrl}}/images/apple-touch-icon-167x167.png">
    <link rel="apple-touch-icon" sizes="152x152" href="{{themeBaseUrl}}/images/apple-touch-icon-152x152.png">
    <link rel="apple-touch-icon" href="{{themeBaseUrl}}/images/apple-touch-icon.png">


    <meta name="theme-color" content="#0b0c0c" />

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:image" content="{{themeBaseUrl}}/images/opengraph-image.png">

    <!--[if gt IE 8]><!--><link href="{{themeBaseUrl}}/stylesheets/main.css"  rel="stylesheet" type="text/css"><!--<![endif]-->
    <!--[if IE 6]><link href="{{themeBaseUrl}}/stylesheets/main-ie6.css" rel="stylesheet" type="text/css" /><![endif]-->
    <!--[if IE 7]><link href="{{themeBaseUrl}}/stylesheets/main-ie7.css" rel="stylesheet" type="text/css" /><![endif]-->
    <!--[if IE 8]><link href="{{themeBaseUrl}}/stylesheets/main-ie8.css" rel="stylesheet" type="text/css" /><![endif]-->

    <script src="{{themeBaseUrl}}/javascripts/vendor/jquery/jquery.min.js"></script>
  </head>

  <body>
    <script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>



    <div id="skiplink-container">
      <div>
        <a href="#content" class="skiplink">{{layout.skipToMainContent}}</a>
      </div>
    </div>

    <div id="global-cookie-message">
        {{{layout.globalCookieMessage}}}
    </div>


    <header role="banner" id="global-header" class="with-proposition">
      <div class="header-wrapper">
        <div class="header-global">
          <div class="header-logo">
            <a href="https://www.gov.uk/" title="{{layout.homepageLink}}" id="logo" class="content">
              <img src="{{themeBaseUrl}}/images/gov.uk_logotype_crown_invert_trans.png" width="36" height="32" alt=""> GOV.UK
            </a>
          </div>

        </div>
        <div class="header-proposition">
          <div class="content">
            <a href="{{formPath}}" id="proposition-name">{{formTitle}}</a>
          </div>
        </div>

      </div>
    </header>




    <div id="global-header-bar"></div>

    <!-- Page content goes here -->
    <div class="site-wrapper">

      <main role="main" id="content">

        <div class="phase-banner">
          <p>
            <strong class="phase-tag">ALPHA</strong>
            <span>{{{layout.alphaBanner}}}</span>
          </p>
        </div>

        {{ > body }}
      </main>
    </div>

    <footer class="group js-footer" id="footer" role="contentinfo">

      <div class="footer-wrapper">


        <div class="footer-meta">
          <div class="footer-meta-inner">

            <h2 class="visuallyhidden">{{layout.supportLinksHeader}}</h2>
						{{{layout.supportLinks}}}

            <div class="open-government-licence">
              <p class="logo"><a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">{{layout.governmentLicenseLogo}}</a></p>

              <p>{{{layout.license}}}</p>

            </div>
          </div>

          <div class="copyright">
            <a href="https://www.nationalarchives.gov.uk/information-management/re-using-public-sector-information/copyright-and-re-use/crown-copyright/">{{layout.copyright}}</a>
          </div>
        </div>
      </div>
    </footer>

    <div id="global-app-error" class="app-error hidden"></div>

    <script src="{{themeBaseUrl}}/javascripts/govuk-template.js"></script>
    <script src="{{themeBaseUrl}}/javascripts/uxforms.js"></script>



    <script>if (typeof window.GOVUK === 'undefined') document.body.className = document.body.className.replace('js-enabled', '');</script>
  </body>
</html>
