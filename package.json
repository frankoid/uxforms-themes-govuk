    "del": "^1.1.1",
    "govuk-elements-sass": "^3.1.2",
    "govuk_frontend_toolkit": "^7.1.0",
    "gulp": "^3.8.11",
    "gulp-bintray": "^0.2.4",
    "gulp-bump": "^1.0.0",
    "gulp-clean": "^0.3.2",
    "gulp-git": "^2.4.2",
    "gulp-preprocess": "^2.0.0",
    "gulp-sass": "^2.2.0",
    "gulp-zip": "^3.0.2",
    "html5shiv": "^3.7.3",
    "http-server": "^0.9.0",
    "jquery": "^1.12.4",
    "json3": "^3.3.2",
    "nodemon": "^1.9.2",
    "properties-reader": "^0.0.15",
    "require-dir": "^0.3.2",
    "run-sequence": "^1.0.2"
  }
