{
  "name": "govuk",
  "version": "0.82.0",
  "description": "A set of css, images and javascript to implement the GOV.UK styles on uxforms",
  "scripts": {
    "server": "http-server ./target -a localhost -p 8000",
    "package": "gulp package",
    "dev": "npm run package && npm run server",
    "watch": "nodemon --exec 'npm run dev' -e js,scss --ignore target/"
  },
  "devDependencies": {
